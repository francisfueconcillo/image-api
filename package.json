{
  "name": "image-api",
  "version": "1.0.0",
  "description": "An Image API to work for an E-commerce Flutter App",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "node index.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "routes/**/*.js",
      "!routes/**/swagger_schema.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@google-cloud/pubsub": "^5.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "firebase-admin": "^13.4.0",
    "form-data": "^4.0.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.1.1"
  }
}
